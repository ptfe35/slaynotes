  <% if page.favorited_by?(current_end_user) %>
  <li>
    <%= link_to page_page_favorites_path(page), method: :delete, remote: true do %>
    <i class="fa fa-heart" aria-hidden="true" style="color: red;"></i>
    <%= page.page_favorites.count %> いいね
    <% end %>
  </li>
  <% else %>
  <li>
    <%= link_to page_page_favorites_path(page), method: :post, remote: true do %>
    <i class="fa fa-heart-o" aria-hidden="true"></i>
    <%= page.page_favorites.count %> いいね
    <% end %>
  </li>
  <% end %>