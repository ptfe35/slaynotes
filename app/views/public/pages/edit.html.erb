<div class="container">
<div class="row">
  <div class="col-12">
    <%= form_with model: @page, local: true do |f| %>
    <%= f.label :name, "ページ名称" %>
    <%= f.text_field :name %>
    <%= f.label :text, "概要" %>
    <%= f.text_area :text, 'data-provider': :summernote %>
    <%= f.label :note_id, "ノート" %>
    <%= f.collection_select :note_id, @notes, :id, :name, selected: @page.note_id %>

      <% if false %>
      <div class="tag">
        <%= f.label :tag, "タグ"  %>
        <%= f.text_field :tag_name, value: @tag_list %>
      </div>
      <% end %>


    <%= f.submit "ページ編集" %>
    <% end %>
  </div>
</div>
</div>
