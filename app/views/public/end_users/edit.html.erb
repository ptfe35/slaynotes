<div class="container">
<div class="row">
  <div class="col-12">
    <%= form_with model: @end_user, local: true do |f| %>
    <%= f.label :name, "名前" %>
    <%= f.text_field :name %>
    <%= f.label :introduction, "紹介文" %>
    <%= f.text_area :introduction %>
    <%= f.label :email, "メールアドレス" %>
    <%= f.text_field :email %>

    <%= f.submit "登録情報変更" %>
    <% end %>
  </div>
</div>
</div>
