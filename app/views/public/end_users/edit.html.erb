<div class="container">
<div class="row">
  <div class="col-12">
    <%= form_with model: @end_user, local: true do |f| %>
    <%= f.label :name, "名前" %><br>
    <%= f.text_field :name %><br>
    <%= f.label :introduction, "紹介文" %><br>
    <%= f.text_area :introduction %><br>
    <%= f.label :email, "メールアドレス" %><br>
    <%= f.text_field :email %><br><br>

    <%= f.submit "登録情報変更", class: "btn btn-success"%>
    <% end %>
  </div>
</div>
</div>
